{
  "name": "bun",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun run uno:dev\" \"bun run liveReload\"",
    "liveReload": "bunx beth-stack",
    "start": "bun run res:build && bun run uno && bun run src/main.ts",
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/model/store/seed.ts",
    "uno": "bunx --bun unocss",
    "uno:dev": "bunx --bun unocss --watch",
    "typecheck": "bunx --bun tsc",
    "format:check:old": "prettier --check .",
    "format:check": "echo iAmPrettyByIntuition",
    "format:old": "prettier --write . --list-different",
    "format": "echo iAmPrettyByIntuition",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "res:build": "rescript",
    "res:dev": "rescript build -w"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@kitajs/ts-html-plugin": "^1.3.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.44.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@unocss/transformer-variant-group": "^0.55.7",
    "better-sqlite3": "^8.7.0",
    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.16",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-isaacscript": "^3.5.7",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "rescript": "11.0.0-rc.5",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.0.13",
    "@elysiajs/cron": "^0.6.0",
    "@elysiajs/static": "^0.6.0",
    "@elysiajs/swagger": "^0.6.2",
    "@iconify-json/logos": "^1.1.38",
    "@iconify-json/lucide": "^1.1.141",
    "@libsql/client": "0.3.5-pre.13",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@lucia-auth/oauth": "^3.5.0",
    "@t3-oss/env-core": "^0.6.1",
    "@tlscipher/holt": "1.1.0",
    "beth-stack": "0.0.33",
    "croner": "^7.0.5",
    "drizzle-orm": "^0.28.6",
    "drizzle-typebox": "^0.1.1",
    "elysia": "^0.7.29",
    "lucia": "^2.7.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  }
}
